  <body id="feed" >
        <!----------------Header from layout-------------------->
        <%= render "/layouts/header"%>
        <!--------------------------------------------------------BODY------------------------------------------------------------------>
        <main class="feed-body">
            <hr>
            <div class="row">
               <!---------Render left tab from admin------->
      ```````<%= render '/admin/leftTab' %>
        <!-------------End Render--------------->
                
                <div class="col-md-8 col-xs-8 photo-body">
                    <div class="container">
                    <br />
                   
                        <div class="row">
          <div class="col-md-10"><h6><strong><%=t('editProfile')%></strong></h6></div>
          <div class="col-md-2"><%= link_to  t("back"), admin_url, class: "btn btn-danger pull-right", :"data-turbolinks" => false %></div>
          </div>
                        <hr>
                        <div class="row">
                            <div class="col-md-5"></div>
                            <div class="col-md-7">
                            <!--Avatar-->
              <img class="edit-profile-photo m-auto" src="<%=@targetAvatar%>" data-toggle="modal" data-target="#avatarModal">
        
            <!---End Avatar------>
            <!----Render avatar model------->
            <%= render "/admin/changeAvatarModel", id: @targetUser.id %>
            <!------End render--------------->
                            
                        </div></div>
                        <br />
                        <div class="row">
                            <div class="col-md-5"></div>
                            <div class="col-md-7"><h6 class="text text-primary"><strong>Basic Information</strong></h6></div>
                        </div>
                        <!-------------------------------Basic Information------------------------------>

                        <%= form_tag("/updateProfile", method: "post", onsubmit: "return validation()") do %>
                      
                            <input type="hidden" name="User[id]" value='<%=@targetUser.id%>'>
                            <!-----------------First Name---------------------->
                            <div class="form-group">
                            <div class="row">
                                <div class="col-md-5">
                                    <label class="col-form-label d-flex justify-content-end" for="User[firstName]"><strong>First Name</strong></label>
                                </div>
                                <div class="col-md-4">
                                    <input type="text" class="form-control" id="first-name" name="User[firstName]" value='<%=@targetUser.firstName%>' placeholder="Your First Name">
                                    <small id="first-name-message" class="form-text text-muted"></small>
                                </div>
                            </div>
                            </div>
                            <!-----------------Last Name---------------------->
                            <div class="form-group">
                            <div class="row">
                                <div class="col-md-5">
                                    <label class="col-form-label d-flex justify-content-end"><strong>Last Name</strong></label>
                                </div>
                                <div class="col-md-4">
                                    <input type="text" class="form-control" id="last-name" name="User[lastName]" value='<%=@targetUser.lastName%>' placeholder="Your Last Name">
                                    <div id="last-name-message" class="form-text text-muted">It should be your last name</div>
                                </div>
                            </div>
                            </div>
                            <!-----------------Email---------------------->
                            <div class="form-group">
                            <div class="row">
                                <div class="col-md-5">
                                    <label class="col-form-label d-flex justify-content-end"><strong>Email</strong></label>
                                </div>
                                <div class="col-md-4">
                                    <input type="text" class="form-control" id="email" name="User[email]" value='<%=@targetUser.email%>' aria-describedby="emailHelp" placeholder="someone@example.com">
                                    <small id="email-message" class="form-text text-muted">It should be your email</small>
                                </div>
                            </div>
                            </div>
                            <!-----------------Password---------------------->
                            <div class="form-group">
                            <div class="row">
                                <div class="col-md-5">
                                    <label class="col-form-label d-flex justify-content-end" for="current-password"><strong>Current password</strong></label>
                                </div>
                                <div class="col-md-4">
                                    <input type="password" class="form-control" id="current-password" name="User[password]" value='<%=@targetUser.password%>' placeholder="12345678">
                                    <small id="password-message" class="form-text text-muted">Your password should have more than 8 characters</small>
                                </div>
                            </div>
                            </div>
                            <div class="row">
                                <div class="col-md-5"></div>
                                <div class="col-md-7">
                                    <button type="submit" value="submit" class="btn btn-success" ><%=t('save')%></button>
                                </div>
                            </div>
                       
                        <%end%>

                        <br />
                        <br />
                        <br />

                <div class="col-md-2 col-xs-2">
                    right
                </div>
            </div>
        
            
        </main>
    </body>
     <!-- Java Script -->
    
</html>